+static void dwmac4_set_mac_loopback(void *ioaddr , bool enable)
+{
+	uint32_t value = rte_read32((uint8_t *)ioaddr + GMAC_CONFIG);
+
+	if (enable)
+		value |= GMAC_CONFIG_LM;
+	else
+		value &= ~GMAC_CONFIG_LM;
+
+	rte_write32(value, (uint8_t *)ioaddr + GMAC_CONFIG);
+}
